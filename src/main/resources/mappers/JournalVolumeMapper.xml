<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.whu.library.modules.mapper.JournalVolumeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.edu.whu.library.modules.pojo.JournalVolume">
        <id column="id" property="id" />
        <result column="journal_id" property="journalId" />
        <result column="seq_volume" property="seqVolume" />
        <result column="editor" property="editor" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, journal_id, seq_volume, editor
    </sql>

</mapper>
